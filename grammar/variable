VariableDeclaration
    VariableAssignment
    VariableDeclarationMutable

VariableDeclarationMutable
    VariableAssignment "mutable"

VariableAssignment
    Expression s "is" s Identifier
    Expression s "is" s "..."
    Expression s "is" s ObjectDeclarationInline

Variable
    Identifier
    ObjectProperty

// note: allows built-in syntax except white space and new line since parsing not ambiguous
Identifier
    NonReservedChars

NonReservedChars
    NonReservedChar
    NonReservedChar NonReservedChars

NonReservedChar
    AnyChar - ReservedChar

AnyChar
    UNICODE_CHAR

ReservedChar
    Whitespace
    Newline